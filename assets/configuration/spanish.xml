        <!-- Punctuation -->
        <dimension>
            <key>punctuation</key>
            <dimensions>
                <dimension>
                    <key>periods</key>
                    <class>CharacterCountDimension</class>
                    <character>.</character>
                </dimension>
                <dimension>
                    <key>commas</key>
                    <class>CharacterCountDimension</class>
