        <!-- Cognitive -->
        <dimension>
            <key>cognitive</key>
            <dimensions>
                <dimension>
                    <key>insight</key>
                    <class>PercentageWordsCapturedFromDictionary</class>
                </dimension>
                <dimension>
                    <key>cause</key>
                    <class>PercentageWordsCapturedFromDictionary</class>
                </dimension>
                <dimension>
                    <key>discrepancies</key>
                    <class>PercentageWordsCapturedFromDictionary</class>
                </dimension>
                <dimension>
                    <key>tentativeness</key>
