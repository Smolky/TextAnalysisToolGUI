<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	
	<directory_folder>assets/dictionaries/en/</directory_folder>
	
	
	<!-- Configuration -->
	<dimensions>
		<!-- Simple, specify the key -->
		<dimension>
			<key>wc</key>
			<class>WordCountDimension</class>
		</dimension>
		<!-- Summary variable dimensions -->
		<dimension>
			<key>summary</key>
			<dimensions>
				<!-- A dictionary dimension with default dictionary -->
				<dimension>
					<key>Analytic</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>Clout</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>Authentic</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- LanguageMetricsDimension -->
		<dimension>
			<key>lng-metrics</key>
			<dimensions>
				<dimension>
					<key>wps</key>
					<class>WordsPerSentenceDimension</class>
				</dimension>
				<dimension>
					<key>Sixltr</key>
					<class>WordsLongerThanNDimension</class>
					<wordlength>6</wordlength>
				</dimension>
				<dimension>
					<key>15ltr</key>
					<class>WordsLongerThanNDimension</class>
					<wordlength>15</wordlength>
				</dimension>
				<dimension>
					<key>Dic</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
					<dictionary>es</dictionary>
				</dimension>
			</dimensions>
		</dimension>
		<!-- FunctionWordsDimension -->
		<dimension>
			<key>lng-function</key>
			<dimensions>
				<dimension>
					<key>personal</key>
					<dimensions>
						<dimension>
							<key>FirstPerson</key>
							<strategy>CompositeStrategyMax</strategy>
							<dimensions>
								<dimension>
									<key>I</key>
									<class>MatchingRegularExpressionsFromDictionary</class>
									<dictionary>personal-pronouns-first-person-singular</dictionary>
								</dimension>
								<dimension>
									<key>We</key>
									<class>MatchingRegularExpressionsFromDictionary</class>
									<dictionary>personal-pronouns-first-person-plural</dictionary>
								</dimension>
							</dimensions>
						</dimension>
						<dimension>
							<key>You</key>
							<class>MatchingRegularExpressionsFromDictionary</class>
							<dictionary>personal-pronouns-second-person</dictionary>
						</dimension>
						<dimension>
							<key>ThirdPerson</key>
							<dimensions>
								<dimension>
									<key>ThirdSingular</key>
									<class>MatchingRegularExpressionsFromDictionary</class>
									<dictionary>personal-pronouns-third-person-singular</dictionary>
								</dimension>
								<dimension>
									<key>ThirdPlural</key>
									<class>MatchingRegularExpressionsFromDictionary</class>
									<dictionary>personal-pronouns-third-person-plural</dictionary>
								</dimension>
							</dimensions>
						</dimension>
					</dimensions>
				</dimension>
				<dimension>
					<key>impersonal</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
					<dictionary>impersonal-pronouns</dictionary>
				</dimension>
			</dimensions>
		</dimension>
		<dimension>
			<key>articles</key>
			<dimensions>
				<dimension>
					<key>prepositions</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>verbs-auxiliary</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>adverbs</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>conjunctions</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>negation</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<dimension>
			<key>Grammar</key>
			<dimensions>
				<dimension>
					<key>adjectives</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>comparatives</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>interrogatives</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>numbers</key>
					<class>NumbersDimension</class>
				</dimension>
				
				<dimension>
					<key>quant</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Affect -->
		<dimension>
			<key>Affect</key>
			<dimensions>
				<dimension>
					<key>positive</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>negative</key>
					<dimensions>
						<dimension>
							<key>anxiety</key>
							<class>MatchingRegularExpressionsFromDictionary</class>
						</dimension>
						<dimension>
							<key>anger</key>
							<class>MatchingRegularExpressionsFromDictionary</class>
						</dimension>
						<dimension>
							<key>sad</key>
							<class>MatchingRegularExpressionsFromDictionary</class>
						</dimension>
					</dimensions>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Social -->
		<dimension>
			<key>Affect</key>
			<dimensions>
				<dimension>
					<key>family</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>female</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>male</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>friendship</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Cognitive -->
		<dimension>
			<key>cognitive</key>
			<dimensions>
				<dimension>
					<key>insight</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>cause</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>discrepancies</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>tentativeness</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>certainty</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>differ</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Perpetual -->
		<dimension>
			<key>perpetual</key>
			<dimensions>
				<dimension>
					<key>feel</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>hear</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>see</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Biological -->
		<dimension>
			<key>bio</key>
			<dimensions>
				<dimension>
					<key>body</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>health</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>ingesting</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>sex</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Biological -->
		<dimension>
			<key>time</key>
			<dimensions>
				<dimension>
					<key>focuspast</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>focuspresent</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>focusfuture</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Relatitivy -->
		<dimension>
			<key>relativity</key>
			<dimensions>
				<dimension>
					<key>motion</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>space</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>time</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Personal -->
		<dimension>
			<key>personal</key>
			<dimensions>
				<dimension>
					<key>death</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>home</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>pleasure</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>money</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>religion</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>work</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Informal speech -->
		<dimension>
			<key>informal</key>
			<dimensions>
				<dimension>
					<key>assent</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>fillers</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>netspeak</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>nonfluencies</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>swear</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Core principles -->
		<dimension>
			<key>core</key>
			<dimensions>
				<dimension>
					<key>affiliation</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>achievement</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>power</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>reward</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
				<dimension>
					<key>risk</key>
					<class>MatchingRegularExpressionsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- Punctuation -->
		<dimension>
			<key>punctuation</key>
			<dimensions>
				<dimension>
					<key>periods</key>
					<class>CharacterCountDimension</class>
					<character>.</character>
				</dimension>
				<dimension>
					<key>commas</key>
					<class>CharacterCountDimension</class>
					<character>,</character>
				</dimension>
				<dimension>
					<key>colons</key>
					<class>CharacterCountDimension</class>
					<character>:</character>
				</dimension>
				<dimension>
					<key>semicolons</key>
					<class>CharacterCountDimension</class>
					<character>:</character>
				</dimension>
				<dimension>
					<key>question</key>
					<class>CharacterCountDimension</class>
					<character>?</character>
				</dimension>
				<dimension>
					<key>exclamations</key>
					<class>CharacterCountDimension</class>
					<character>!</character>
				</dimension>
				<dimension>
					<key>dashes</key>
					<class>CharacterCountDimension</class>
					<character>-</character>
				</dimension>
				<dimension>
					<key>quotes</key>
					<class>CharacterCountDimension</class>
					<character>"</character>
				</dimension>
				<dimension>
					<key>apostrophe</key>
					<class>CharacterCountDimension</class>
					<character>'</character>
				</dimension>
				<dimension>
					<key>parenthesis</key>
					<class>CharacterCountDimension</class>
					<character>()</character>
				</dimension>

				<dimension>
					<key>emoticons</key>
					<class>EmoticonsDimension</class>
				</dimension>
			</dimensions>
		</dimension>
	</dimensions>
</configuration>