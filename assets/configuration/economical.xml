<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    
    <!-- Use spanish words -->
    <directory_folder>../assets/dictionaries/es/</directory_folder>
    
    
    <!-- Configuration -->
    <dimensions> 
        
        <!-- Word count -->
        <dimension>
            <key>words-count</key>
            <class>WordCountDimension</class>
            <description>Counts how many words there are in the text</description>
        </dimension>
        
        
        <!-- Sentence count -->
        <dimension>
            <key>sentences-count</key>
            <class>SentenceCountDimension</class>
            <description>Counts how many dimensions there are in the text</description>
        </dimension>
        
        
        <!-- Words per sentence -->
        <dimension>
            <key>words-per-sentence</key>
            <class>WordsPerSentenceDimension</class>
            <description>The average number of words per sentence</description>
        </dimension>
        
        
        <!-- Words length average -->
        <dimension>
            <key>words-length-avg</key>
            <class>WordAverageLengthDimension</class>
            <description>The average length of the words</description>
        </dimension>
        
        
        <!-- Total words in the dictionary -->
        <dimension>
            <key>words-in-dictionary</key>
            <class>MatchingRegularExpressionsFromDictionary</class>
            <dictionary>es</dictionary>
            <description>The number of words inside a dictionary</description>
        </dimension>
        
        
        <!-- % of the words of the text that belong to a dictionary -->
        <dimension>
            <key>words-in-dictionary-avg</key>
            <class>PercentageWordsCapturedFromDictionary</class>
            <dictionary>es</dictionary>
            <description>The percentage of words inside a dictionary</description>
        </dimension>
        
        
